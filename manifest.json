{
  "name": "Review monitor",
  "version": "0.1",
  "version_name": "Proof of concept",
  "manifest_version": 2,
  "description": "Monitors user interaction in GitHub code review",
  "permissions": ["*://github.com/*"],
  "icons": {"16": "icon16.bmp",
            "48": "icon48.bmp",
            "128": "icon128.bmp"},
  "content_scripts": [
    {
      "matches": ["*://github.com/*"],
      "js": ["monitor.js"]
    }
  ]
}